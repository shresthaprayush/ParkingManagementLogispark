<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_margin="10dp"
    android:layout_height="wrap_content">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:id="@+id/linearlayoutps"
        android:layout_marginTop="15dp"
        android:orientation="vertical">

        <TextView
            android:id="@+id/texviewcheckOutdatedialouge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="2dp"
            android:text="Check Out : 2078-1012"
            android:textColor="@color/blackdark"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/texviewiteminTimedialouge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="2dp"
            android:text="Check In : 23:45:78"
            android:textColor="@color/blackdark"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/textviewVechileNumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="10dp"
            android:layout_centerHorizontal="true"
            android:layout_marginEnd="5dp"
            android:layout_marginBottom="5dp"
            android:text="Vechile Number : Ba 1kha 2020"
            android:textColor="@color/blackdark"
            android:textSize="16sp"
            />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/lblstock"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="Type : "
                android:textColor="@color/blackdark"
                android:textSize="16sp" />


            <TextView
                android:id="@+id/textViewVechileType"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_toRightOf="@+id/lblstock"
                android:layout_marginLeft="5dp"
                android:padding="2dp"
                android:text=" 25 "
                android:textColor="@color/blackdark"
                android:textSize="16sp" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/lblprice"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="Rate : "
                android:textColor="@color/blackdark"
                android:textSize="16sp" />


            <TextView
                android:layout_width="wrap_content"
                android:id="@+id/textviewrate"
                android:text=" 25 "
                android:layout_toEndOf="@id/lblprice"
                android:textColor="@color/blackdark"
                android:padding="2dp"
                android:textSize="16sp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_toRightOf="@id/lblprice" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/lblduration"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="Duration : "
                android:textColor="@color/blackdark"
                android:textSize="16sp" />


            <TextView
                android:id="@+id/textViewVechileDuration"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_toRightOf="@+id/lblduration"
                android:layout_marginLeft="5dp"
                android:padding="2dp"
                android:text=" 25 "
                android:textColor="@color/blackdark"
                android:textSize="16sp" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/lbldiscount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="Discount : "
                android:textColor="@color/blackdark"
                android:textSize="16sp" />


            <TextView
                android:id="@+id/textViewVechileDiscount"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_toRightOf="@+id/lbldiscount"
                android:layout_marginLeft="5dp"
                android:padding="2dp"
                android:text=" 25 "
                android:textColor="@color/blackdark"
                android:textSize="16sp" />

        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/lblsubTotal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="Sub Total : "
                android:textColor="@color/blackdark"
                android:textSize="16sp" />


            <TextView
                android:id="@+id/textViewVechileSubTotal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_toRightOf="@+id/lblsubTotal"
                android:layout_marginLeft="5dp"
                android:padding="2dp"
                android:text=" 25 "
                android:textColor="@color/blackdark"
                android:textSize="16sp" />

        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/lblAccommodation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="Accommodation : "
                android:textColor="@color/blackdark"
                android:textSize="16sp" />


            <TextView
                android:id="@+id/textViewVechileAccommodation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_toRightOf="@+id/lblAccommodation"
                android:layout_marginLeft="5dp"
                android:padding="2dp"
                android:text=" 25 "
                android:textColor="@color/blackdark"
                android:textSize="16sp" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/lblWashing"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="Washing : "
                android:textColor="@color/blackdark"
                android:textSize="16sp" />


            <TextView
                android:id="@+id/textViewVechileWashing"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_toRightOf="@+id/lblWashing"
                android:layout_marginLeft="5dp"
                android:padding="2dp"
                android:text=" 25 "
                android:textColor="@color/blackdark"
                android:textSize="16sp" />

        </RelativeLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_weight="1"
            android:layout_marginTop="5dp"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/lblGrandTotal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="2dp"
                android:text="Grand Total : "
                android:textColor="@color/blackdark"
                android:textSize="20sp" />


            <TextView
                android:id="@+id/textViewVechilGrandTotal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_toRightOf="@+id/lblGrandTotal"
                android:layout_marginLeft="5dp"
                android:padding="2dp"
                android:text=" 25 "
                android:textColor="@color/blackdark"
                android:textSize="20sp" />

        </RelativeLayout>




    </LinearLayout>


    <RelativeLayout
        android:id="@+id/closeitemdialouge"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:layout_below="@+id/linearlayoutps"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="5dp"
        android:background="@drawable/background_color"
        android:gravity="center_horizontal"
        android:text="Close">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:text="Close"
            android:textColor="@color/blackdark"
            android:textSize="18sp" />
    </RelativeLayout>
</RelativeLayout>